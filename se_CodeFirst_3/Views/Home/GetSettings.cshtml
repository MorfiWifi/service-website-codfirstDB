@model IEnumerable<se_CodeFirst_3.Models.SettingViewModel>

@{
    ViewBag.Title = "GetSettings";
}

<h2>GetSettings</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SettingName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SettingValue)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SettingName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SettingValue)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
